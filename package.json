{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run strings && node scripts/start.js",
    "build": "npm run strings && npm run types && node scripts/build.js && node scripts/http",
    "test": "node scripts/test.js",
    "lint:check": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "format:check": "stylelint 'src/**/*.{ts,tsx}' && prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "npm run lint:check && npm run format:check",
    "format": "npm run lint:fix && npm run format:fix",
    "gql:check": "npx apollo client:check",
    "gql:types": "rm -rf src/types/gql && mkdir -p src/types/gql && npx apollo client:download-schema src/types/gql/schema.json && npx apollo client:codegen --target typescript --outputFlat src/types/gql && npx graphql-codegen && node src/gql/bundleTypes && prettier --write \"./src/types/gql/*.ts\"",
    "ws:types": "typerpc schema socket.picast.app -o src/types/ws/index.ts",
    "types": "npm run gql:types && npm run ws:types",
    "strings": "node scripts/bundleLocales"
  },
